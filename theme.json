{
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "name": "Black",
          "slug": "black",
          "color": "Black"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "White"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "SkyBlue"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "MediumTurquoise"
        },
        {
          "name": "Page",
          "slug": "page",
          "color": "Snow"
        },
        {
          "name": "Links",
          "slug": "links",
          "color": "SlateBlue"
        },
        {
          "name": "Links: Hover",
          "slug": "links-hover",
          "color": "DarkSlateBlue"
        },
        {
          "name": "Body",
          "slug": "body",
          "color": "#111111"
        },
        {
          "name": "Headings",
          "slug": "headings",
          "color": "#333333"
        },
        {
          "name": "Light Text",
          "slug": "light-text",
          "color": "SlateGray"
        },
        {
          "name": "Call to Action",
          "slug": "call-to-action",
          "color": "var(--header-cta-button-color)"
        },
        {
          "name": "Gray Wash",
          "slug": "gray-wash",
          "color": "rgba(0, 0, 0, .1)"
        },
        {
          "name": "Highlight",
          "slug": "highlight",
          "color": "rgba(255, 255, 0, .5)"
        }
      ],
      "gradients": [
        {
          "name": "Primary Gradient",
          "slug": "cpt-primary",
          "gradient": "linear-gradient(-135deg, var(--wp--preset--color--primary) -90%, #efefef 100%)"
        },
        {
          "name": "Secondary Gradient",
          "slug": "cpt-secondary",
          "gradient": "linear-gradient(-135deg, var(--wp--preset--color--primary) -90%, #efefef 100%)"
        }
      ]
    },
    "layout": {
			"contentSize": "1100px",
      "wideSize": "1320px"
		},
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "Box Shadow",
          "slug": "box-shadow",
          "shadow": "1px 1px 5px rgba(0, 0, 0, .25), 2.5px 2.5px 15px rgba(0, 0, 0, .1)"
        },
        {
          "name": "Text Shadow",
          "slug": "text-shadow",
          "shadow": "1px 1px 0 rgba(0, 0, 0, 0.25), 0 0 25px Black"
        }
      ]
    },
    "spacing": {
      "customSpacingSize": true,
      "spacingScale": {
        "operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 2.5,
				"unit": "rem"
      }
		},
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fluid": true,
      "letterSpacing": false,
      "textDecoration": false,
      "fontFamilies": [
        {
          "fontFamily": "\"Albert Sans\", sans-serif",
          "name": "Albert Sans",
          "slug": "albert-sans",
            "fontFace": [
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "400",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-regular.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "400",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "100",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-100.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "100",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-100italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "200",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-200.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "200",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-200italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "300",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-300.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "300",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-300italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "500",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-500.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "500",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-500italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "700",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-700.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "700",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-700italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "800",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-800.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "800",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-800italic.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "normal",
                "fontWeight": "900",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-900.woff2"
                ]
              },
              {
                "fontFamily": "Albert Sans",
                "fontStyle": "italic",
                "fontWeight": "900",
                "src": [
                  "file:./assets/fonts/albert-sans/albert-sans-v1-latin-900italic.woff2"
                ]
              }
            ]
        }
      ],
      "fontSizes": [
        {
          "name": "XS",
          "fluid": {
            "min": ".64rem",
            "max": ".8rem"
          },
					"size": ".8rem",
					"slug": "xs"
				},
				{
          "name": "Small",
          "fluid": {
            "min": ".8rem",
            "max": "1rem"
          },
					"size": "1rem",
					"slug": "sm"
				},
        {
          "name": "Normal",
          "fluid": false,
					"size": "1.25rem",
					"slug": "normal"
				},
				{
          "name": "Medium",
          "fluid": {
            "min": "1.25rem",
            "max": "1.563rem"
          },
					"size": "1.563rem",
					"slug": "md"
				},
				{
          "name": "Large",
          "fluid": {
            "min": "1.563rem",
            "max": "1.953rem"
          },
					"size": "1.953rem",
					"slug": "lg"
				},
				{
          "name": "XL",
          "fluid": {
            "min": "1.953rem",
            "max": "2.441rem"
          },
					"size": "2.441rem",
					"slug": "xl"
				},
        {
          "name": "XXL",
          "fluid": {
            "min": "2.441rem",
            "max": "3.052rem"
          },
					"size": "3.052rem",
					"slug": "xxl"
				},
        {
          "name": "Huge",
          "fluid": {
            "min": "3.052rem",
            "max": "4.768rem"
          },
					"size": "3.815rem",
					"slug": "huge"
				}
			]
    },
    "custom": {
      "border": {
        "normal": "var(--wp--custom--border--width--normal) solid var(--wp--custom--border--color--normal)",
        "buttons": "var(--wp--custom--border--width--buttons) solid var(--wp--custom--border--color--buttons)",
        "color": {
          "normal": "LightSlateGray",
          "buttons": "currentColor"
        },
        "radius": {
          "normal": "0",
          "buttons": "9999px",
          "fields": "2px"
        },
        "width": {
          "normal": "1px",
          "buttons": "2px",
          "buttons-sm": "1.5px",
          "buttons-xs": "1px"
        }
      },
      "typography": {
        "line-height": {
					"sm": "1.15",
					"normal": "1.5"
				}
      }
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--page-color)",
      "text": "var(--wp--preset--color--body)"
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--40)",
      "padding": {
				"right": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--40)"
			}
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--albert-sans)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "var(--wp--custom--typography--line-height--normal)"
    },
    "blocks": {
      "core/code": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--sm)",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      },
      "core/latest-comments": {
        "spacing": {
          "padding": {
            "left": 0
          }
        }
      },
      "core/list": {
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--60)"
          }
        },
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      },
      "core/paragraph": {
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      },
      "core/post-title": {
        "color": {
          "text": "var(--wp--preset--color--headings)"
        },
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
        }
      },
      "core/pullquote": {
        "spacing": {
          "padding": false
        },
        "elements": {
          "blockquote": {
            "spacing": {
              "padding": false
            }
          },
          "cite": {
            "color": {
              "text": "var(--wp--preset--color--light-text)"
            },
            "typography": {
              "fontSize": "80%",
              "lineHeight": "var(--wp--custom--typography--line-height--sm)"
            }
          }
        }
      },
      "core/quote": {
        "border": {
          "left": {
            "color": "var(--wp--preset--color--gray-wash)",
            "style": "solid",
            "width": "5px"
          }
        },
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--30)"
          }
        },
        "elements": {
          "cite": {
            "color": {
              "text": "var(--wp--preset--color--light-text)"
            },
            "typography": {
              "fontSize": "80%",
              "lineHeight": "var(--wp--custom--typography--line-height--sm)"
            }
          }
        }
      },
      "core/search": {
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
        },
        "elements": {
          "input": {
            "button": {
              "border": {
                "color": "currentColor"
              },
              "color": {
                "text": "var(--wp--preset--color--body)"
              }
            },
            "spacing": {
              "padding": ".25em .5em"
            }
          }
        }
      },
      "core/site-title": {
        "color": {
          "text": "var(--wp--preset--color--headings)"
        },
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
        }
      }
    },
    "elements": {
      "button": {
        "border": {
          "color": "var(--wp--custom--border--color--buttons)",
          "radius": "var(--wp--custom--border--radius--buttons)",
          "style": "solid",
          "width": "var(--wp--custom--border--width--buttons)"
        },
        "color": {
          "background": "transparent",
          "text": "var(--wp--preset--color--body)"
        },
        "spacing": {
					"padding": ".5em 1em"
				},
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--sm)",
          "textDecoration": "none"
        }
      },
			"h1": {
				"typography": {
          "fontSize": "var(--wp--preset--font-size--xxl)",
					"fontWeight": "bold",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
				}
			},
			"h2": {
				"typography": {
          "fontSize": "var(--wp--preset--font-size--xl)",
					"fontWeight": "bold",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
				}
			},
			"h3": {
				"typography": {
          "fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "bold",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
				}
			},
			"h4": {
				"typography": {
          "fontSize": "var(--wp--preset--font-size--md)",
					"fontWeight": "bold",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
				}
			},
			"h5": {
				"typography": {
          "fontSize": "inherit",
					"fontWeight": "bold",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)",
          "textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
          "fontSize": "var(--wp--preset--font-size--sm)",
					"fontWeight": "normal",
          "lineHeight": "var(--wp--custom--typography--line-height--sm)",
          "textTransform": "uppercase"
				}
			},
      "caption": {
        "typography": {
          "lineHeight": "var(--wp--custom--typography--line-height--sm)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--links)"
        },
        "typography": {
          "textDecoration": "underline"
				}
      }
		}
  }
}
